<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Vector.PopupMenu" parent="Vector.PopupMenuBase" />

    <style name="VectorEmptyImageView">
        <item name="android:visibility">visible</item>
    </style>
    <style name="radioButtonStyle" parent="Widget.MaterialComponents.CompoundButton.RadioButton">
        <item name="android:fontFamily">@font/svn_poppins_regular</item>
        <item name="android:background">@drawable/background_edit_text</item>
        <item name="android:textSize">16sp</item>
        <item name="buttonTint">@color/riotx_accent</item>
    </style>
    <style name="Vector.TextInputLayout.Login" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="boxCornerRadiusBottomStart">10dp</item>
        <item name="boxCornerRadiusBottomEnd">10dp</item>
        <item name="boxCornerRadiusTopStart">10dp</item>
        <item name="boxCornerRadiusTopEnd">10dp</item>
        <item name="boxBackgroundColor">@color/green_white</item>
        <item name="boxStrokeWidth">0dp</item>
        <item name="boxStrokeWidthFocused">2dp</item>
<!--        <item name="boxStrokeColor">@color/boxstroke_outline</item>-->
    </style>
    <style name="Vector.TextInputLayout" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="boxCornerRadiusBottomStart">8dp</item>
        <item name="boxCornerRadiusBottomEnd">8dp</item>
        <item name="boxCornerRadiusTopStart">8dp</item>
        <item name="boxCornerRadiusTopEnd">8dp</item>
        <item name="boxBackgroundColor">@color/white</item>
    </style>

    <style name="Vector.DropDownLayout" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu">
        <item name="boxCornerRadiusBottomStart">8dp</item>
        <item name="boxCornerRadiusBottomEnd">8dp</item>
        <item name="boxCornerRadiusTopStart">8dp</item>
        <item name="boxCornerRadiusTopEnd">8dp</item>
        <item name="endIconTint">@color/color_dropdown_icon</item>
        <item name="boxBackgroundColor">@color/white</item>
    </style>
    <style name="Vector.DropDownLayout.disable" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu">
        <item name="boxCornerRadiusBottomStart">8dp</item>
        <item name="boxCornerRadiusBottomEnd">8dp</item>
        <item name="boxCornerRadiusTopStart">8dp</item>
        <item name="boxCornerRadiusTopEnd">8dp</item>
        <item name="boxBackgroundColor">@color/white</item>
    </style>
    <style name="Vector.DropDownLayout.Login" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu">
        <item name="boxCornerRadiusBottomStart">10dp</item>
        <item name="boxCornerRadiusBottomEnd">10dp</item>
        <item name="boxCornerRadiusTopStart">10dp</item>
        <item name="boxCornerRadiusTopEnd">10dp</item>
        <item name="boxBackgroundColor">@color/green_white</item>
        <item name="boxStrokeWidth">0dp</item>
        <item name="boxStrokeWidthFocused">2dp</item>
        <item name="endIconTint">@color/riotx_accent</item>
    </style>
    <style name="RatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/ratingbar_full</item>
        <item name="android:indeterminateDrawable">@drawable/ratingbar_full</item>
        <item name="android:minHeight">23dip</item>
        <item name="android:maxHeight">50dip</item>
    </style>
    <style name="CommonTextTitleRadio">
        <item name="android:fontFamily">@font/svn_poppins_regular</item>
        <item name="android:textColor">@color/riotx_text_color</item>
        <item name="android:textSize">16sp</item>
    </style>
    <style name="VectorCalender" parent="@style/CalendarDatePickerStyle">
        <item name="colorAccent">@color/riotx_text_color_login</item>
        <item name="colorControlActivated">@color/riotx_text_color_login</item>
    </style>

</resources>